{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "andreasjr-tools/trading-card",
  "version": "1.0.0",
  "title": "Trading Card",
  "category": "theme",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "getDataFrom": {
      "type": "string",
      "default": "currentPost"
    },
    "postId": {
      "type": "number",
      "default": null
    },
    "customData": {
      "type": "object",
      "default": {
        "title": "",
        "description": "",
        "image": {},
        "url": ""
      }
    }
  },
  "usesContext": [
    "postId",
    "postType"
  ],
  "textdomain": "andreasjr-tools",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css",
    "wp-block-buttons",
    "wp-block-button"
  ],
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}